pipeline{
    agent none
    stages{
        stage('Example'){
            steps{
                echo 'hello world'
            }
        }
        stage('master'){
            steps{
                 if[$branch='feature']:then
                         echo 'feature branch'
                 fi
            }
        }
        stage('feature'){
             steps{
                if[$branch='feature'];then
                      echo 'feature branch'
                fi
             }
        }
    }
}
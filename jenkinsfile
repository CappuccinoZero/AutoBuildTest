pipeline{
    agent none
    stages{
        stage('Example'){
            steps{
                echo 'hello world okk'
            }
        }
        stage('Master Branch'){
            when{
                branch 'master'
            }
            steps{
                echo 'master commit'
            }
        }
        stage('Feature Branch'){
            when{
                branch 'feature'
            }
            steps{
                echo 'feature commit'
            }
        }
        stage('Get'){
            steps{
                script{
                    def str = 'world'
                    print "hello ${str}"
                }
            }
        }
    }
}